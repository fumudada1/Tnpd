@using System.Xml
@using tnpd.Models
@using TnpdModels
@{
    List<NodeLink> nodeLinks = ViewBag.nodeLinks;
 }

<div class="row">
    <div class="col-12">
        <div class="alertlist">

          
  @foreach (var link in nodeLinks)
        {
          
            <div class="alert alert-primary">
                <i class="icon-line-circle-plus"></i> <a href="@link.url" target="@link.Target" title="@link.title">@link.Tooltip</a>
            </div>
        }


        </div>
                                
    </div>
</div>

   
       
   




