
@using Tnpd.Models
@model TnpdModels.TrafficSMSCarInfoReject

@{
    ViewBag.Title = "不通過原因編輯編輯";
}

<h2>不通過原因編輯編輯</h2>

    @using (Html.BeginForm()) {

    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>TrafficSMSCarInfoReject</legend>

        @Html.HiddenFor(model => model.Id)

        <div class="editor-label">
            @Html.LabelFor(model => model.Subject)
        </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.Subject,new { placeholder=ModelsAttributeHelper<TnpdModels.TrafficSMSCarInfoReject>.GetDisplayName("Subject") })
           
        </div>

            @Html.HiddenFor(model => model.ListNum)
            @Html.HiddenFor(model => model.Poster)
        @Html.HiddenFor(model => model.initOrg)
        @Html.HiddenFor(model => model.UpdateOrg)
        



            @Html.HiddenFor(model => model.InitDate)


            @Html.HiddenFor(model => model.UpdateDate)
        <p>
            <input type="submit" value="更新" /> <input type="button" onclick="history.back()" value="回到上一頁" />
        </p>
    </fieldset>
}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")

}
