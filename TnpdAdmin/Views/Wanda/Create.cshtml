@using TnpdModels
@model TnpdModels.Wanda

@{
    ViewBag.Title = "婦幼安全警示地點新增";
}

<h2>婦幼安全警示地點新增</h2>

    @using (Html.BeginForm()) {

    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    @ViewBag.Message
    <fieldset>
        <legend>Wanda</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.Subject)

        </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.Subject,new { placeholder=ModelsAttributeHelper<TnpdModels.Wanda>.GetDisplayName("Subject") })
            @Html.ValidationMessageFor(model => model.Subject)
       
            
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.StationInfoId)

        </div>
        <div class="editor-field">
            @Html.DropDownList("StationInfoId")
            @Html.ValidationMessageFor(model => model.StationInfoId)
       
            
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.beginning)

        </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.beginning,new { placeholder=ModelsAttributeHelper<TnpdModels.Wanda>.GetDisplayName("beginning") })
            @Html.ValidationMessageFor(model => model.beginning)
       
            
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.destination)

        </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.destination,new { placeholder=ModelsAttributeHelper<TnpdModels.Wanda>.GetDisplayName("destination") })
            @Html.ValidationMessageFor(model => model.destination)
       
            
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Article)

        </div>
        <div class="editor-field">
            @Html.TextAreaFor(model => model.Article,new { @class = "ckeditor" })
            @Html.ValidationMessageFor(model => model.Article)
       
            
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Status)

        </div>
        <div class="editor-field">
            @Html.DropDownListFor(model => model.Status, new SelectList(Enum.GetValues(typeof(TnpdModels.BooleanType))))
            @Html.ValidationMessageFor(model => model.Status)
       
            
        </div>

       

        <p>
            <input type="submit" value="新增" /><input type="button" onclick="history.back()" value="回到上一頁" />
        </p>
    </fieldset>
}



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")

   <script src="@Url.Content("~/Scripts/ckeditor/ckeditor.js")" type="text/javascript"></script>
    
}
