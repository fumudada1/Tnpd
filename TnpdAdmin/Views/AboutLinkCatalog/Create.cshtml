@using TnpdModels
@model TnpdModels.AboutLinkCatalog

@{
    ViewBag.Title = "新增網網相連";
}

<h2>新增網網相連</h2>

    @using (Html.BeginForm()) {

    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    @ViewBag.Message
    <fieldset>
        <legend>AboutLinkCatalog</legend>

       

        <div class="editor-label">
            @Html.LabelFor(model => model.WebSiteId)

        </div>
        <div class="editor-field">
            @Html.DropDownList("WebSiteId")
            @Html.ValidationMessageFor(model => model.WebSiteId)
       
            
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Subject)

        </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.Subject,new { placeholder=ModelsAttributeHelper<TnpdModels.AboutLinkCatalog>.GetDisplayName("Subject") })
            @Html.ValidationMessageFor(model => model.Subject)
       
            
        </div>

             @Html.Hidden("ListNum","0")
     

        <p>
            <input type="submit" value="新增" /><input type="button" onclick="history.back()" value="回到上一頁" />
        </p>
    </fieldset>
}



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")


}
