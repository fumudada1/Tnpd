
@using TnpdModels
@model TnpdModels.AboutLinkCatalog

@{
    ViewBag.Title = "編輯網網相連";
}

<h2>編輯網網相連</h2>

    @using (Html.BeginForm()) {

    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>AboutLinkCatalog</legend>

        @Html.HiddenFor(model => model.Id)

     
        <div class="editor-label">
            @Html.LabelFor(model => model.WebSiteId)
        </div>
        <div class="editor-field">
            @Html.DropDownList("WebSiteId")
            @Html.ValidationMessageFor(model => model.WebSiteId)
           
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Subject)
        </div>
        <div class="editor-field">
            @Html.TextBoxFor(model => model.Subject,new { placeholder=ModelsAttributeHelper<TnpdModels.AboutLinkCatalog>.GetDisplayName("Subject") })
           
        </div>

            @Html.HiddenFor(model => model.ListNum)
            @Html.HiddenFor(model => model.Poster)
        @Html.HiddenFor(model => model.initOrg)

     

            @Html.HiddenFor(model => model.InitDate)
      

            @Html.HiddenFor(model => model.UpdateDate)
        <p>
            <input type="submit" value="更新" /> <input type="button" onclick="history.back()" value="回到上一頁" />
        </p>
    </fieldset>
}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")

}
